<?
	Object.defineProperty(Object.prototype, "forIn", {
		value: function(fn, self) {
			self = self || this;

			Object.keys(this).forEach(function(key, index) {
				var value = this[key];
				var len = Object.keys(self).length;
				fn.call(self, key, value, index, len);
			}, this);
		}
	});
?>

<? function list(k, data, index, len){ ?>
	<? if(data.type == 'file'){ ?>
		<? if(index != 0){ ?></li><? } ?>
		<li class="pageTree_item">
			<? if(data.finish){ ?>
				<a href="<?- data.url ?>">
			<? } else { ?>
				<div>
			<? } ?>

				<div class="pageTree_itemInner">
					<p class="pageTree_name"><?- data.name ?></p>
					<p class="pageTree_num"><?- data.id ?></p>
				</div>
			<? if(data.finish == 1){ ?>
				</a>
			<? } else { ?>
				</div>
			<? } ?>
		<? if(index == len - 1){ ?></li><? } ?>
	<? } else { ?>
		<ul class="pageTree_cate">
			<? data.children.forIn(function(k, data, index, len){ list(k, data, index, len) }) ?>
		</ul>
		<? if(index == len - 1 && k != 'rootDir'){ ?></li><? } ?>
	<? } ?>
<? } ?>

<div class="pageTree">
	<? data.forIn(function(k, data, index, len){ list(k, data, index, len) }) ?>
</div><!-- /.pageTree -->


